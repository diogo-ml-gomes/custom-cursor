{
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "script",
        "ecmaFeatures": {
            "globalReturn": true,
            "impliedStrict": true,
            "jsx": true
        }
    },
    "rules": {
        "no-debugger": 2,
        "accessor-pairs": 2,
        "array-bracket-spacing": 2,
        "array-callback-return": 2,
        "arrow-body-style": 2,
        "arrow-parens": 2,
        "arrow-spacing": 2,
        "block-scoped-var": 2,
        "block-spacing": 2,
        "brace-style": 2,
        "callback-return": 2,
        "camelcase": 2,
        "capitalized-comments": 2,
        "class-methods-use-this": 2,
        "comma-spacing": 2,
        "no-dupe-else-if": 2,
        "no-duplicate-case": 2,
        "no-duplicate-imports": 2,
        "no-else-return": 2,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-empty-function": 2,
        "no-empty-pattern": 2,
        "no-extra-bind": 2,
        "no-extra-label": 2,
        "no-extra-boolean-cast": 2,
        "no-eq-null": 2,
        "no-template-curly-in-string": 2,
        "no-unmodified-loop-condition": 2,
        "no-unreachable": 2,
        "no-unsafe-negation": 2,
        "no-useless-constructor": 2,
        "no-global-assign": 2,
        "no-label-var": 2,
        "one-var": 2,
        "no-alert": 2,
        "no-console": 1,
        "no-compare-neg-zero": 2,
        "no-const-assign": 2,
        "strict": 1
    },
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "mocha": true,
        "jquery": false,
        "es6": true,
        "webextensions": true,
        "es2017": true,
        "es2020": true,
        "prototypejs": true
    }
}